{
	"name": "frontend",
	"private": true,
	"type": "module",
	"scripts": {
		"watch": "vite",
		"build": "vite build",
		"storybook-dev": "nodemon --verbose --watch src --ext \"mdx,ts,vue\" --ignore \"*.stories.ts\" --exec \"pnpm build-storybook-pre && pnpm exec storybook dev -p 6006 --ci\"",
		"build-storybook-pre": "(tsc -p .storybook || echo done.) && node .storybook/generate.js && node .storybook/preload-locale.js && node .storybook/preload-theme.js",
		"build-storybook": "pnpm build-storybook-pre && storybook build --webpack-stats-json storybook-static",
		"chromatic": "chromatic",
		"test": "vitest --run --globals --environment happy-dom",
		"test-and-coverage": "vitest --run --coverage --globals --environment happy-dom",
		"typecheck": "vue-tsc --noEmit",
		"eslint": "eslint --quiet \"src/**/*.{ts,vue}\"",
		"lint": "pnpm typecheck && pnpm eslint"
	},

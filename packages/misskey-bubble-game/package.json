{
  "type": "module",
  "name": "misskey-bubble-game",
  "version": "0.0.1",
  "types": "./built/index.d.ts",
  "exports": {
    ".": {
      "import": "./built/index.js",
      "types": "./built/index.d.ts"
    },
    "./*": {
      "import": "./built/*",
      "types": "./built/*"
    }
  },
  "scripts": {
    "build": "pnpm run build:tsc",
    "build:tsc": "tsc --outDir built --declaration true --declarationMap true",
    "watch": "nodemon -w src -e ts,js,cjs,mjs,json --exec \"pnpm run build:tsc\"",
    "typecheck": "tsc --noEmit",
    "format": "biome check --write",
    "lint": "pnpm typecheck && biome check"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/matter-js": "catalog:",
    "@types/node": "catalog:",
    "@types/seedrandom": "catalog:",
    "esbuild": "catalog:",
    "execa": "catalog:",
    "nodemon": "catalog:",
    "typescript": "catalog:"
  },
  "files": [
    "built"
  ],
  "dependencies": {
    "esbuild": "catalog:",
    "eventemitter3": "catalog:",
    "glob": "catalog:",
    "matter-js": "catalog:",
    "seedrandom": "catalog:"
  }
}
